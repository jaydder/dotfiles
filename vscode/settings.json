{
  "editor.fontFamily": "Hasklig Semibold",
  "editor.fontSize": 13.5,
  "editor.lineHeight": 20,
  "editor.fontLigatures": true,
  "editor.fontWeight": "normal",
  "editor.renderLineHighlight": "gutter",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1.1,
  "terminal.integrated.fontFamily": "monospace",
  "editor.tabSize": 2,
  "editor.rulers": [
    85
  ],
  "editor.formatOnSave": false,
  "editor.autoIndent": "full",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "[robot]": {
    "editor.fontLigatures": false,
    "editor.tabSize": 4
  },
  "extensions.ignoreRecommendations": true,
  "workbench.editor.highlightModifiedTabs": true,
  "git.enableSmartCommit": true,
  "editor.parameterHints.enabled": false,
  "editor.autoClosingQuotes": "languageDefined",
  "editor.autoClosingBrackets": "languageDefined",
  "workbench.colorCustomizations": {
    "editorLineNumber.activeForeground": "#ffffff",
    "editorLineNumber.foreground": "#555555",
    "editor.lineHighlightBackground": "#ffffff00",
    "editor.lineHighlightBorder": "#ff000000",
    "tab.activeBorderTop": "#ff000000",
    "terminal.background": "#000000"
  },
  "explorer.confirmDragAndDrop": false,
  "breadcrumbs.enabled": false,
  "files.trimTrailingWhitespace": false,
  "editor.dragAndDrop": true,
  "files.insertFinalNewline": false,
  "editor.cursorStyle": "line",
  "editor.suggestSelection": "first",
  "editor.trimAutoWhitespace": false,
  "editor.showUnused": true,
  "explorer.compactFolders": false,
  "workbench.startupEditor": "welcomePage",
  "files.trimFinalNewlines": false,
  "python.languageServer": "Jedi",
  "[python]": {
    "editor.rulers": [
      79
    ],
    "editor.formatOnSave": false,
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.python"
  },
  "python.autoComplete.addBrackets": true,
  "python.linting.enabled": true,
  "python.formatting.provider": "autopep8",
  "python.formatting.blackArgs": [
    "--line-length",
    "79"
  ],
  "python.autoUpdateLanguageServer": true,
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "editor.linkedEditing": true,
  "explorer.confirmDelete": false,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.cursorBlinking": "smooth",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "terminal.integrated.cursorBlinking": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "git-rebase-todo": "gitlens.rebase"
  },
  "terminal.integrated.enablePersistentSessions": true,
  "python.pythonPath": "/bin/python3",
  "files.exclude": {
    "**/__pycache__": true,
    "**.env": true,
    "**/.vscode": true
  },
  "terminal.integrated.defaultProfile.linux": "bash",
  "python.defaultInterpreterPath": "/bin/python3",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "git.confirmSync": false,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 10,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.activityBar.visible": false,
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "United Ubuntu",
  }